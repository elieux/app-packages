noextract=("${source[@]%%::*}")

pkgver() {
    #find . -mindepth 2 -maxdepth 2 -name 'install.rdf' -printf '%TY.%Tm.%Td\n' | sort | tail -1
    find . -mindepth 1 -maxdepth 1 -name '*.xpi' -execdir bash -c 'unzip -l "{}" | tail -n +4 | head -n -2' \; | sed -r 's/[ ]+/\t/g' | cut -f 3 | tr '-' '.' | sort | tail -1
}

prepare() {
    for f in *.xpi
    do
        case "${f}" in
        bengali-bangladesh-dictionary-13660.xpi)
            unzip -o "${f}" -d "${f%%.xpi}" || true # dictionaries/:  ucsize 238 <> csize 0 for STORED entry
            ;;
        *)
            unzip -o "${f}" -d "${f%%.xpi}"
            ;;
        esac
    done
}

package() {
    a="${pkgdir}/apps${_apparch}/${_dirname}"
    mkdir -p "${a}" "${a}/extensions"
    for f in *.xpi
    do
        f="${f%%.xpi}"
        ids="$(cat "${f}/install.rdf" | (grep 'dictionaries.addons.mozilla.org' || true); cat "${f}/install.rdf" | grep 'em:id' | grep '@')"
        id="$(echo "${ids}" | head -1 | sed 's/em:id=//' | tr -d '"=' | sed -r 's/^[^>]+>//' | sed -r 's/<[^<]+$//' | tr -d '[[:space:]]')"
        echo "${f}: ${id}"
        mkdir "${a}/extensions/${id}"
        cp -r "${f}"/* "${a}/extensions/${id}"
    done
}
