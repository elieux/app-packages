_realname=deno
_choconame=deno
pkgbase="app-${_realname}"
pkgname=("app-x86_64-${_realname}")
epoch=1
pkgver=2.5.6
pkgrel=1
pkgdesc="A secure runtime for JavaScript and TypeScript"
url="https://deno.land/"
arch=('any')
license=('MIT')
provides=("${pkgbase}")
options=(!strip)
source=("deno-x86_64-pc-windows-msvc.zip::https://github.com/denoland/deno/releases/download/v${pkgver}/deno-x86_64-pc-windows-msvc.zip")
sha512sums=('405ba99d8cf8bf5d565858e8764ca9d5e705bb065a1f11babbf481bb2437189de1df4d7102c8cc51ee581a3dd8cb9f87143335f5b825478160669b57fa633172')

package() {
    local a="${pkgdir}/apps/${_realname}"
    mkdir -p "${a}"
    cp "deno.exe" "${a}"
}
