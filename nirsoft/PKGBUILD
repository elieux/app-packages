prepare() {
    cd "${srcdir}"

    for util in *.zip
    do
        unzip -o "${util}" -d "${util%%.zip}"
        for f in "${util%%.zip}"/*
        do
            n=$(echo "${f}" | tr 'A-Z' 'a-z')
            if test "${f}" != "${n}"
            then
                mv "${f}" "${n}"
            fi
        done
    done
}

package() {
    mkdir -p "${pkgdir}/apps${_apparch}/${_realname}"
    mkdir -p "${pkgdir}/docs${_apparch}/${_realname}"

    for dir in $1
    do
        dir="${dir%%.zip}"
        util="${dir##*/}"
        util="${util%%-x64}"

        for ext in exe dll frm frx vbp vbw
        do
            find "${dir}" -name '*.'"${ext}" -not -name 'regsvr32.exe' -execdir cp "{}" "${pkgdir}/apps${_apparch}/${_realname}" \;
        done
        for file in "${util}.txt" 'readme.txt'
        do
            find "${dir}" -name "${file}" -execdir cp "{}" "${pkgdir}/docs${_apparch}/${_realname}/${util}.txt" \;
        done
        find "${dir}" -name '*.chm' -execdir cp "{}" "${pkgdir}/docs${_apparch}/${_realname}" \;

        extras=$(find "${dir}" -mindepth 1 -regextype posix-extended -not -regex '.+\.(exe|dll|ocx|frm|frx|vbp|vbw|chm)' -not -name 'readme.txt' -not -name "${util}.txt")
        for f in ${extras}
        do
            mkdir -p "${pkgdir}/docs${_apparch}/${_realname}/${util}"
            cp "${f}" "${pkgdir}/docs${_apparch}/${_realname}/${util}"
        done
    done
}

package_app-i686-nirsoft() {
    _apparch=32
    files="$(find "${srcdir}" -mindepth 1 -maxdepth 1 -name '*.zip' -not -name '*x64*')"
    package "${files}"
    rm "${pkgdir}/apps${_apparch}/${_realname}/runfromprocess-x64.exe"
}

package_app-x86_64-nirsoft() {
    _apparch=64
    files="$(find "${srcdir}" -mindepth 1 -maxdepth 1 -name '*x64*.zip')"
    package "${files}"
    cp "${srcdir}/runfromprocess/runfromprocess-x64.exe" "${pkgdir}/apps${_apparch}/${_realname}/runfromprocess.exe"
}
