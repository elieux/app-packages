_realname=deno
_choconame=deno
pkgbase="app-${_realname}"
pkgname=("app-x86_64-${_realname}")
epoch=1
pkgver=1.14.1
pkgrel=1
pkgdesc="A secure runtime for JavaScript and TypeScript"
url="https://deno.land/"
arch=('any')
license=('MIT')
provides=("${pkgbase}")
options=(!strip)
source=("deno-x86_64-pc-windows-msvc.zip::https://github.com/denoland/deno/releases/download/v${pkgver}/deno-x86_64-pc-windows-msvc.zip")
sha512sums=('c5f1c8ddcdc4469baed3c43f3269081055af7fe9ef7316fcc523d36d79c92e5fb6a20574ebace6a1be913403fd60ab640fed3c3fa48ac57301a88bbdf5cfe509')

package() {
    local a="${pkgdir}/apps/${_realname}"
    mkdir -p "${a}"
    cp "deno.exe" "${a}"
}
