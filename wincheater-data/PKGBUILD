_realname=wincheater-data
_dirname=wincheater
_apparch=32
pkgbase="app-${_realname}"
pkgname=("app-i686-${_realname}")
pkgver=165
pkgrel=2
pkgdesc="Texts for WinCheater"
url="http://www.cheater.cz/winch/win_download_knihovny.htm"
arch=('any')
license=('custom')
makedepends=("p7zip" "unrar")
provides=("${pkgbase}")
options=(!strip)
source=("texty.exe::http://www.cheater.cz/download/upd/${pkgver}/texty.exe")
md5sums=('9128a53aeb2530744a2ba8e1b82c6931')

package() {
    a="${pkgdir}/apps${_apparch}/${_dirname}"
    mkdir -p "${a}"
    cp *.sct "${a}"
}
