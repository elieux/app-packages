_lc() {
    for f in "${@}"
    do
        n="$(echo "${f}" | tr 'A-Z' 'a-z')"
        if test "${f}" != "${n}"
        then
            mv "${f}" "${n}"
        fi
    done
}

_build() {
    for f in *win${_apparch}*.msi
    do
        msiexec -a "${f}" "TARGETDIR=$(cygpath -wa "install${_apparch}")" -qn
    done
    _lc "install${_apparch}/Program Files/TortoiseSVN"/*
    _lc "install${_apparch}/PFiles/TortoiseSVN"/*
}

build() {
    for _apparch in 32 64
    do
        _build
    done
}

_package() {
    a="${pkgdir}/apps${_apparch}/${_realname}"
    mkdir -p "${a}"
    cp -r "install${_apparch}/PFiles/TortoiseSVN"/* "install${_apparch}/Program Files/TortoiseSVN"/* "${a}"
    rm -r "${a}/common"
    rm "${a}/bin"/api-ms-win-*.dll "${a}/bin"/mfc*140*.dll "${a}/bin/ucrtbase.dll" "${a}/bin"/{concrt,msvcp,vcamp,vccorlib,vcomp,vcruntime}140.dll

    d="${pkgdir}/docs${_apparch}/${_realname}"
    mkdir -p "${d}"
    mv "${a}"/*.txt "${a}/bin"/*.{url,chm} "${a}/languages"/*.chm "${d}"
    cp *.pdf "${d}"
}

package_app-i686-tortoisesvn() {
    depends=("app-i686-tortoise-overlays")
    _apparch=32
    _package
}

package_app-x86_64-tortoisesvn() {
    depends=("app-x86_64-tortoise-overlays")
    _apparch=64
    _package
}
