noextract=("${source[@]%%::*}")

pkgver() {
    #find . -mindepth 2 -maxdepth 2 -name 'install.rdf' -printf '%TY.%Tm.%Td\n' | sort | tail -1
    find . -mindepth 1 -maxdepth 1 -name '*.xpi' -execdir bash -c 'unzip -l "{}" | tail -n +4 | head -n -2' \; | sed -r 's/[ ]+/\t/g' | cut -f 3 | tr '-' '.' | sort | tail -1
}

prepare() {
    for f in *.xpi
    do
        case "${f}" in
        bengali-bangladesh-dictionary-13660.xpi)
            unzip -q -o "${f}" -d "${f%%.xpi}" || true # dictionaries/:  ucsize 238 <> csize 0 for STORED entry
            ;;
        *)
            unzip -q -o "${f}" -d "${f%%.xpi}"
            ;;
        esac
    done
}

_package() {
    a="${pkgdir}/apps${_apparch}/${_dirname}"
    mkdir -p "${a}" "${a}/browser/extensions"
    for f in *.xpi
    do
        f="${f%%.xpi}"
        id="$(
            cd "${f}"
            test ! -f "install.rdf" || python3 -c '
import xml.etree.ElementTree as ET
file = open("install.rdf")
desc = ET.parse(file).getroot().find("{http://www.w3.org/1999/02/22-rdf-syntax-ns#}Description")
id = desc.find("{http://www.mozilla.org/2004/em-rdf#}id")
if id is not None:
    print(id.text)
else:
    print(desc.get("{http://www.mozilla.org/2004/em-rdf#}id"))
            '
            test ! -f "manifest.json" || python3 -c '
import json
file = open("manifest.json")
id = json.load(file)["applications"]["gecko"]["id"]
print(id)
            '
        )"
        echo "${f}: ${id}"
        mkdir "${a}/browser/extensions/${id}"
        cp -r "${f}"/* "${a}/browser/extensions/${id}"
    done
}

package_app-i686-firefox-dictionaries() {
    depends=("app-i686-firefox")
    _apparch=32
    _package
}

package_app-x86_64-firefox-dictionaries() {
    depends=("app-x86_64-firefox")
    _apparch=64
    _package
}
