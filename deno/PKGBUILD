_realname=deno
_choconame=deno
pkgbase="app-${_realname}"
pkgname=("app-x86_64-${_realname}")
epoch=1
pkgver=1.44.0
pkgrel=1
pkgdesc="A secure runtime for JavaScript and TypeScript"
url="https://deno.land/"
arch=('any')
license=('MIT')
provides=("${pkgbase}")
options=(!strip)
source=("deno-x86_64-pc-windows-msvc.zip::https://github.com/denoland/deno/releases/download/v${pkgver}/deno-x86_64-pc-windows-msvc.zip")
sha512sums=('1b86ca41425ca3eaa38fce4fa822b5d86c1bddb175716cdb7ca4a3b6eae6c3a6b4b0c74cb6579c67b58e20b4b107b042cb94c28707584289e7b8f2ab4eda7bb7')

package() {
    local a="${pkgdir}/apps/${_realname}"
    mkdir -p "${a}"
    cp "deno.exe" "${a}"
}
