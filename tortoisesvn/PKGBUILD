_lc() {
    local d f n

    for f in "${@}"
    do
        d="$(dirname "${f}")"
        f="$(basename "${f}")"
        n="$(echo "${f}" | tr 'A-Z' 'a-z')"
        if test "${f}" != "${n}"
        then
            mv "${d}/${f}" "${d}/${n}"
        fi
    done
}

build() {
    for f in *win64*.msi
    do
        msiexec -a "${f}" "TARGETDIR=$(cygpath -wa "install")" -qn
    done
    _lc "install/Program Files/TortoiseSVN"/*
    _lc "install/PFiles/TortoiseSVN"/*
}

package() {
    a="${pkgdir}/apps/${_realname}"
    mkdir -p "${a}"
    cp -r "install/PFiles/TortoiseSVN"/* "install/Program Files/TortoiseSVN"/* "${a}"
    rm -r "${a}/common"
    rm "${a}/bin"/api-ms-win-*.dll "${a}/bin"/mfc*140*.dll "${a}/bin/ucrtbase.dll" "${a}/bin"/{concrt,msvcp,vcamp,vccorlib,vcomp,vcruntime}140.dll
    cp *.pdf "${a}"
}
