_realname=deno
_choconame=deno
pkgbase="app-${_realname}"
pkgname=("app-x86_64-${_realname}")
epoch=1
pkgver=1.27.2
pkgrel=1
pkgdesc="A secure runtime for JavaScript and TypeScript"
url="https://deno.land/"
arch=('any')
license=('MIT')
provides=("${pkgbase}")
options=(!strip)
source=("deno-x86_64-pc-windows-msvc.zip::https://github.com/denoland/deno/releases/download/v${pkgver}/deno-x86_64-pc-windows-msvc.zip")
sha512sums=('e3a17ad76662562dabc798aa2c8bd0667ec50e6d2638edf4defcaaec0d53d6a12642cc98093f81841d36d6ff84926231e0ecd0877a3533d28b4dfa1e29cec5e1')

package() {
    local a="${pkgdir}/apps/${_realname}"
    mkdir -p "${a}"
    cp "deno.exe" "${a}"
}
