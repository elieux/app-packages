noextract=("abwsx1.exe")

_pickarch() {
    local d f n

    for f in "${@}"
    do
        d="$(dirname "${f}")"
        f="$(basename "${f}")"
        n="$(echo "${f}" | sed -r -e 's/_[0-9]+[.]/./' -e 's/[.]duplicate[0-9]+$//')"
        case "$(file "${d}/${f}")" in
        *x86-64*|*assembly*) mv -f "${d}/${f}" "${d}/${n}" ;;
        *) rm "${d}/${f}" ;;
        esac
    done
}

prepare() {
    rm -fR "abwsx1.dir"
    echo u | 7z x -bd "abwsx1.exe" -o"abwsx1.dir" > /dev/null
}

pkgver() {
    powershell -command '(Get-Item "abwsx1.exe").VersionInfo.ProductVersionRaw.ToString()'
}

package() {
    local a="${pkgdir}/apps/${_realname}"
    mkdir -p "${a}"
    cp -R "abwsx1.dir"/* "${a}"
    cp -f *.lng "${a}/lang"
    rm -R "${a}"/'$PLUGINSDIR'
    _pickarch "${a}"/*.{exe,dll} "${a}/adn"/*.{exe,dll}
}
