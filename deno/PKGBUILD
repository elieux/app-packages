_realname=deno
_choconame=deno
pkgbase="app-${_realname}"
pkgname=("app-x86_64-${_realname}")
epoch=1
pkgver=1.15.3
pkgrel=1
pkgdesc="A secure runtime for JavaScript and TypeScript"
url="https://deno.land/"
arch=('any')
license=('MIT')
provides=("${pkgbase}")
options=(!strip)
source=("deno-x86_64-pc-windows-msvc.zip::https://github.com/denoland/deno/releases/download/v${pkgver}/deno-x86_64-pc-windows-msvc.zip")
sha512sums=('71afefa80db3c6fe3d43be632f68a4ed0364bdd49fec1e4fdac107a3c11b48b946dd1bd8b81fc345d59cc9ecab64c8305f9b7032346d3c414814ac59fe352df6')

package() {
    local a="${pkgdir}/apps/${_realname}"
    mkdir -p "${a}"
    cp "deno.exe" "${a}"
}
