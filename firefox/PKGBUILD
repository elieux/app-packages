noextract=("${source[@]%%::*}")

_lc() {
    local d f n

    for f in "${@}"
    do
        d="$(dirname "${f}")"
        f="$(basename "${f}")"
        n="$(echo "${f}" | tr 'A-Z' 'a-z')"
        if test "${f}" != "${n}"
        then
            mv "${d}/${f}" "${d}/${n}"
        fi
    done
}

prepare() {
    for _apparch in 32 64
    do
        7z x -y -bd "FirefoxSetup${_apparch}.exe" -o"FirefoxSetup${_apparch}"
        _lc "FirefoxSetup${_apparch}/core"/*
    done
}

_package() {
    a="${pkgdir}/apps${_apparch}/${_realname}"
    mkdir -p "${a}"
    cp -r "FirefoxSetup${_apparch}/core"/* "${a}"
    sed -ri -e '/(VCRUNTIME|MSVCP)140.dll/d' -e '/api-ms-win-[0-9a-z-]+.dll/d' "${a}/dependentlibs.list"
    rm "${a}"/api-ms-win-*.dll "${a}"/ucrtbase.dll "${a}"/{vcruntime,msvcp}140.dll "${a}"/maintenanceservice{,_installer}.exe
    for f in *."${_apparch}.xpi"
    do
        f="${f##*/}"
        f="${f%%.${_apparch}.xpi}"
        cp "${f}.${_apparch}.xpi" "${a}/browser/extensions/langpack-${f}@firefox.mozilla.org.xpi"
    done
}

package_app-i686-firefox() {
    _apparch=32
    _package
}

package_app-x86_64-firefox() {
    _apparch=64
    _package
}
